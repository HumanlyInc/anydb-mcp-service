{
  "name": "anydb-mcp-service",
  "version": "1.0.0",
  "description": "MCP server for AnyDB Template creation and management",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "anydb-mcp-service": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
